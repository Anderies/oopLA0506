package LearningS7overriding;

class Heroes{
	public void status() {
		System.out.println("I Have Agi, Int and Str Status");
	}
}

class Tank extends Heroes{
	
}

public class overriding3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Tank t = new Tank();
		t.status();
	}

}
