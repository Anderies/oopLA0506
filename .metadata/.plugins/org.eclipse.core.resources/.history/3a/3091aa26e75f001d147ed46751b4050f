package LearningS7overriding;

class Persona {
	String nama;
	String address;
	String phoneNumber;
	String emailAddress;

//	Persona(String nama, String address, String phoneNumber, String emailAddress) {
//		this.nama = nama;
//		this.address = address;
//		this.phoneNumber = phoneNumber;
//		this.emailAddress = emailAddress;
//	}

	public String toString() {
		return "Nama " + nama + "\naddress: " + address 
				+ " phone_number: " + phoneNumber + " email_address: " + emailAddress;
	}
}

class Murid extends Persona {
	final String status = "Freshman";

	Murid() {
		
	}
	
	public String toString() {
		return super.toString() + "Status: " +status;
	}

}

class Pegawai extends Persona {
	String office;
	String salary;
	String dateHired;

}

class Employee {

}

class Dosen extends Employee {
	String officeHours;
	String jabatanAkademik;
}

class Staff extends Employee {
	String title;
}

public class soal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Persona p = new Persona();
		p.toString();

	}

}
